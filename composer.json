{
    "name": "simple-as-fuck/laravel-performance-log",
    "description": "Laravel service for logging slow parts of application.",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/log": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/events": "^8.0",
        "simple-as-fuck/php-validator": "^0.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.2"
    },
    "suggest": {
        "sentry/sentry-laravel": "Allows receive logs with performance warning and make statistic on them, send notification, ..."
    },
    "autoload": {
        "psr-4": {"SimpleAsFuck\\LaravelPerformanceLog\\": "src/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleAsFuck\\LaravelPerformanceLog\\Provider\\PackageProvider"
            ]
        }
    }
}
